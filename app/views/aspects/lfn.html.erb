<div id="bd" align="center">
  <object data="data:application/x-silverlight-2," type="application/x-silverlight-2" width="100%" height="890">
    <param name="source" value="/data/ActivityStream.xap" />
    <param name="onError" value="onSilverlightError" />
    <param name="background" value="white" />
    <param name="minRuntimeVersion" value="4.0.50826.0" />
    <param name="autoUpgrade" value="true" />
    <param name="scalemode" value="Stretch" />           
    <a href="http://go.microsoft.com/fwlink/?LinkID=149156&v=4.0.50826.0" style="text-decoration: none">
        <img src="http://go.microsoft.com/fwlink/?LinkId=161376" alt="Get Microsoft Silverlight" style="border-style: none" />
    </a>
  </object>
  <iframe id="_sl_historyFrame" style="visibility: hidden; height: 0px; width: 0px; border: 0px"></iframe>
</div>
<script type="text/javascript">
  function stretch()
  {
    var obj=document.getElementById("bd");
    var parent=obj.parentNode.parentNode;
    parent.style.width="100%";
    obj.parentNode.style.width="100%";
    document.body.style.padding="0";
    document.getElementById("header_body").style.margin="0";
  }
  window.onload=stretch;
</script>
